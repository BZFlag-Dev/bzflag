[!header.tmpl]
	<div>&nbsp;</div>
	<table class="titleBox"><tr><td>Current players</td></tr></table>
	<table class="playerlist">
	[*START Players]
	<tr><td>[$PlayerID]</td><td>[$Callsign]</td></tr>
	[*END Players]</td></tr>
	<tr><td>No Players</li>
	[*EMPTY Players]
	</table>
  <div>&nbsp;</div>
  <table class="titleBox"><tr><td>Recent Chat</td></tr></table></br>
  <table class="chatTable">
  <tr><th>Timestamp</th><th>From</th><th>To</th><th>Message</th></tr>
  [*START ChatLines]
	<tr>
	<td>[$ChatLineTime]</td><td>[$ChatLineUser]
	[?IF ChatLineIsToTeam]
		([$ChatLineTeam])
	[?END ChatLineIsToTeam]
	</td>
	<td>[$ChatLineTo]</td>
	<td>[$ChatLineText]</td>
	</tr>
  [*END ChatLines]
      <tr><td>No chat</td></tr>
  [*EMPTY ChatLines]
  </table>
  
  <div>&nbsp;</div>
  <table class="titleBox"><tr><td>Server Info</td></tr></table>
  <p>Map Name: [$MapFile]</p>
  <table class="minibox"><tr><td>TeamScores</td></tr></table>
  <table class="teamScoreTable">
  <tr><th>Team</th><th>Score</th><th>Wins</th><th>Losses</th><tr>
  <tr>
  <td>Red</td>
  <td>[$RedTeamScore]</td>
  <td>[$RedTeamWins]</td>
  <td>[$RedTeamLosses]</td>
  </tr>
  
  <tr>
  <td>Green</td>
  <td>[$GreenTeamScore]</td>
  <td>[$GreenTeamWins]</td>
  <td>[$GreenTeamLosses]</td>
  </tr>
  
  <tr>
  <td>Blue</td>
  <td>[$BlueTeamScore]</td>
  <td>[$BlueTeamWins]</td>
  <td>[$BlueTeamLosses]</td>
  </tr>
  
  <tr>
  <td>Purple</td>
  <td>[$PurpleTeamScore]</td>
  <td>[$PurpleTeamWins]</td>
  <td>[$PurpleTeamLosses]</td>
  </tr>
</table>
[!footer.tmpl]
