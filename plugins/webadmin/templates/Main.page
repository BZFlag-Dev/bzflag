[!header.tmpl]
	</hr>
	   <h2>Current players</h2>
		<ul>
	[*START Players]
		  <li><input type="checkbox" id="player_[$PlayerID]"
					 name="player" value="[$PlayerID]">
			<label for="player_[$PlayerID]">[$Callsign]</label></li>
	[*END Players]
		  <li>No Players</li>
	[*EMPTY Players]
  </hr>
  <h2>Recent Chat</h2>
  <br>
  <table border="0" align="left">
  <tr><th>Timestamp</th><th>From</th><th>To</th><th>Message</th></tr>
  [*START ChatLines]
	<tr>
	<td>[$ChatLineTime]</td><td>[$ChatLineUser]
	[?IF ChatLineIsToTeam]
		([$ChatLineTeam])
	[?END ChatLineIsToTeam]
	</td>
	<td>[$ChatLineTo]</td>
	<td>[$ChatLineText]</td>
	</tr>
  [*END ChatLines]
      <tr><td>No chat</td></tr>
  [*EMPTY ChatLines]
  </table>
  <br>
  </hr>
  <h2>Server Info</h2>
  <p> Map Name: [$MapFile]</p>
  <p><h3>TeamScores</h3></p>
  <p>Red:[$RedTeamScore]</p>
  <p>Green:[$GreenTeamScore]</p>
  <p>Blue:[$BlueTeamScore]</p>
  <p>Purple:[$PurpleTeamScore]</p>

[!footer.tmpl]
