<table class="authHeader" width ="100%">
    <tr>
        [*START Navigation]
            <td class= [?IF IsCurrentPage] "authSelect" [?ELSE IsCurrentPage] "authHeader" [?END IsCurrentPage]>
            <a href="[$BaseURL][$PageName]">[$PageTitle]</a>
            </td>
        [*END Navigation]
            <td>NAV ERROR!</td>
        [*EMPTY Navigation]
        <td class="authHeader">
            <a id="logout" href="[$BaseURL]logout">Log Out</a>
        </td>
    </tr>
</table>
[?IF Error]
<p class="error">[$Error]</p>
[?END Error]
